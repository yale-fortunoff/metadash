{
  "name": "metadash",
  "version": "1.1.0",
  "private": false,
  "homepage": "/metadash",
  "dependencies": {
    "airtable": "^0.11.6",
    "d3": "^5.9.2",
    "numeral": "^2.0.6",
    "pym.js": "^1.3.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-scripts": "^5.0.1",
    "semiotic": "^1.19.7",
    "unidecode": "^0.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node download_airtable.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "document": "jsdoc src/Data --destination ./book/data-api",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --dotfiles=true",
    "deploy:bot": "gh-pages -d build --dotfiles=true",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm test",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "jsdoc": "^3.6.1",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "sass": "^1.79.4",
    "sass-loader": "^13.3.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
