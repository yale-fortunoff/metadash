{
  "name": "metadash",
  "version": "0.1.0",
  "private": false,
  "homepage": "/metadash-staging",
  "dependencies": {
    "airtable": "^0.11.6",
    "babel-polyfill": "^6.26.0",
    "d3": "^5.9.2",
    "gh-pages": "^2.0.1",
    "node-sass": "^8.0.0",
    "numeral": "^2.0.6",
    "pym.js": "^1.3.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-scripts": "^5.0.1",
    "semiotic": "^1.19.7",
    "unidecode": "^0.1.8",
    "yargs": "^14.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "node download_airtable.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "document": "jsdoc src/Data --destination ./book/data-api",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build --dotfiles=true",
    "deploy-bot": "gh-pages -d build --dotfiles=true",
    "deploy-dev": "node publish.js publish --remote origin",
    "deploy-prod": "node publish.js publish --remote production",
    "jsontocsv": "node json_to_csv.js",
    "csvtojson": "node csv_to_json.js",
    "help": "gh-pages --help"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 1 ie version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "devDependencies": {
    "csv": "^6.2.8",
    "jsdoc": "^3.6.1",
    "jsonexport": "^3.2.0",
    "url-loader": "^1.1.2"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
